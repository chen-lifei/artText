{
  "name": "500d-demo",
  "description": "500d",
  "author": "huangcanhui@500d.me",
  "private": true,
  "scripts": {
    "dev": "cross-env CONF_ENV=dev node --max_old_space_size=4096 server",
    "test": "cross-env NODE_ENV=production CONF_ENV=dev pm2 start server.js -i 2 --max-memory-restart 1024M --no-daemon",
    "ptest": "cross-env NODE_ENV=production CONF_ENV=ptest pm2 start server.js -i 2 --max-memory-restart 1024M --no-daemon",
    "onlinetest": "cross-env NODE_ENV=production CONF_ENV=onlinetest pm2 start server.js -i 2 --max-memory-restart 1024M --no-daemon",
    "grayproduct": "cross-env NODE_ENV=production CONF_ENV=grayproduct pm2 start server.js -i 2 --max-memory-restart 1024M --no-daemon",
    "start": "cross-env NODE_ENV=production CONF_ENV=production pm2 start server.js -i 2 --max-memory-restart 1024M --no-daemon",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.1",
    "cssdom": "^1.0.23",
    "echarts": "^4.3.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "intersection-observer": "^0.12.0",
    "jquery": "^2.2.4",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.8.1",
    "less-loader": "^4.0.6",
    "lru-cache": "^4.1.1",
    "qrcode": "^1.2.0",
    "route-cache": "^0.4.4",
    "serve-favicon": "^2.4.5",
    "socket.io-client": "^2.2.0",
    "svg-path-parser": "^1.1.0",
    "vue": "^2.5.3",
    "vue-clipboard2": "^0.1.0",
    "vue-color": "^2.7.0",
    "vue-drr": "^1.0.8",
    "vue-lazyload": "^1.3.3",
    "vue-meta-info": "^0.1.7",
